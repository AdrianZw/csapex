<package>
  <name>robot_detection</name>
  <version>0.0.1</version>
  <description>The robot_detection package provides nodes to learn and recognize robots in camera images</description>

  <maintainer email="sebastian.buck@uni-tuebingen.de">buck</maintainer>

  <license>BSD</license>
  <author email="sebastian.buck@uni-tuebingen.de">buck</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>angles</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>cv_bridge</build_depend>
  <build_depend>background_subtraction</build_depend>
  <build_depend>dynamic_reconfigure</build_depend>
  <build_depend>utils</build_depend>
  <build_depend>vision_evaluator</build_depend>
  <build_depend>tf_conversions</build_depend>

  <run_depend>angles</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>cv_bridge</run_depend>
  <run_depend>background_subtraction</run_depend>
  <run_depend>dynamic_reconfigure</run_depend>
  <run_depend>utils</run_depend>
  <run_depend>vision_evaluator</run_depend>
  <run_depend>tf_conversions</run_depend>

  <export>
    <vision_evaluator plugin="${prefix}/plugins.xml" />
  </export>

</package>


